</center>
<% @listings.each do |l| %>
	<div class="jumbotron">
		<div class="container-fluid">
			<div class="row">
								
						<div class="col-md-4 col-md-offset-2">
							
								<%= image_tag("1.jpg", size: "560x315", class: "listing-image") %>
									
							
						</div>
						<div class="col-md-4 col-md-offset-0">
							
									<em><h2><strong><%= l.name %></strong></h2></em>
									<p><%= l.short_description %></p>
									<%= link_to "See More ->", view_listing_path(l) %>
							
				
						</div>
					
			</div>
		</div>
	</div>
<% end %>

